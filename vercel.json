{
  "builds": [
    {
      "src": "prod.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "prod.js"
    },{
      "src": "/intro",
      "dest": "frontend/static/js/index.js"
    },{
      "src": "/end",
      "dest": "frontend/static/js/index.js"
    }
  ]
}